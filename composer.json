{
    "name": "worksome/route-checker",
    "description": "Checks if routes have valid controllers",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^9.46|^10.0",
        "illuminate/routing": "^9.46|^10.0",
        "illuminate/support": "^9.46|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Worksome\\RouteChecker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oliver Nybroe",
            "email": "oliver@worksome.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Worksome\\RouteChecker\\RouteCheckerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "worksome/coding-style": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "worksome/coding-style": true
        }
    }
}
